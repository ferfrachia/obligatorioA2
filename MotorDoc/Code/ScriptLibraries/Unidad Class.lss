'******************************************************************************************************************	
	' Tincho en accion !
	Public Function buscarPadreHijos(Byval pUnidadCreadora As String) As Integer
		
		Dim unidad As String
		Dim uCreadora As String
		Dim uArchiva As String
		Dim res As Integer
		Dim docUnidadAux As NotesDocument
		res = 0
		
		unidad = Strright(pUnidadCreadora, "_") ' Esta es la unidad que creó el exp
		
		'Veo si no es la misma unidad la que archiva que la que creo el exp.
		If Not unidad = docUnidad.GetItemValue("sNroUnidad")(0) Then
			Set ViewUnidades = dbOrg.GetView(VISTA_BUSQUEDAUNIDADES)			
			If Not ViewUnidades Is Nothing Then
				Set docUnidadAux = ViewUnidades.GetDocumentByKey(unidad, True)
				uCreadora = docUnidadAux.GetItemValue("sCodVisUnidad")(0)
				uArchiva = docUnidad.GetItemValue("sCodVisUnidad")(0)
				If uCreadora = uArchiva Then
					res = 1
				Else
					res = 0
				End If
			End If
		Else
			res = 1
		End If
		buscarPadreHijos=res
	End Function	
End Class