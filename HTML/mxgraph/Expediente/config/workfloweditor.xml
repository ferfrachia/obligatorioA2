<mxEditor>
	<include name="/mxgraph/Expediente/config/wfeditor-commons.xml"/>
	<ui>
		<resource basename="/mxgraph/Expediente/resources/"/>
		<add as="graph" element="graph"
			style="left:255px;right:20px;top:130px;bottom:40px;height:500px"/>
		<add as="status" element="status"
			style="height:20px;bottom:20px;left:20px;right:20px"/>
		<add as="toolbar" element="toolbarContainer"
			style="padding:5px;padding-top:8px;padding-right:0px;"/>
	</ui>
	<mxGraph as="graph">
		<include name="/mxgraph/Expediente/config/wfgraph-commons.xml"/>
	</mxGraph>
	<mxDefaultToolbar as="toolbar">
		<include name="/mxgraph/Expediente/config/wftoolbar-commons.xml"/>
	</mxDefaultToolbar>
</mxEditor>