<mxDefaultToolbar>
	<add as="select" mode="select" icon="/mxgraph/javascript/examples/editors/images/select.gif"/>
	<add as="pan" mode="pan" icon="/mxgraph/javascript/examples/editors/images/pan.gif"/>
	<br/><hr/>
	<add as="undo" action="undo" icon="/mxgraph/javascript/examples/editors/images/undo.gif"/>
	<add as="redo" action="redo" icon="/mxgraph/javascript/examples/editors/images/redo.gif"/>
	<br/><hr/>
	<add as="cut" action="cut" icon="/mxgraph/javascript/examples/editors/images/cut.gif"/>
	<add as="copy" action="copy" icon="/mxgraph/javascript/examples/editors/images/copy.gif"/>
	<add as="paste" action="paste" icon="/mxgraph/javascript/examples/editors/images/paste.gif"/>
	<add as="delete" action="delete" icon="/mxgraph/javascript/examples/editors/images/delete.gif"/>
	<br/><hr/>
	<add as="Inicio" template="inicio" icon="/mxgraph/javascript/examples/editors/images/symbols/small_event.gif"/>
	<add as="Fin" template="fin" icon="/mxgraph/javascript/examples/editors/images/symbols/small_terminate.gif"/>
	<add as="Unidad" template="unidad" icon="/mxgraph/javascript/examples/editors/images/rectangle.gif"/>
	<add as="Cualquier unidad" template="u_cualquiera" icon="/mxgraph/javascript/examples/editors/images/rounded.gif"/>
	<add as="Unidad inicial" template="u_inicial" style="ellipse" icon="/mxgraph/javascript/examples/editors/images/ellipse.gif"/>
	<add as="Unidad superior" template="u_superior" icon="/mxgraph/javascript/examples/editors/images/hexagon.gif"/>
	<add as="Accion" template="accion" icon="/mxgraph/javascript/examples/editors/images/gear.gif"/>
	<add as="Grupo" template="u_grupo" icon="/mxgraph/javascript/examples/editors/images/press.gif"/>
	<add as="Bifurcacion" template="bifurcacion" icon="/mxgraph/javascript/examples/editors/images/symbols/small_fork.gif"/>
	<add as="connect" mode="connect" icon="/mxgraph/javascript/examples/editors/images/straight.gif"><![CDATA[
		function (editor)
		{
			if (editor.defaultEdge != null)
			{
				editor.defaultEdge.style = 'straightEdge';
			}
		}
	]]></add>
	<br/><hr/>
	<add as="zoomIn" action="zoomIn" icon="/mxgraph/javascript/examples/editors/images/zoomin.gif"/>
	<add as="zoomOut" action="zoomOut" icon="/mxgraph/javascript/examples/editors/images/zoomout.gif"/>
	<add as="actualSize" action="actualSize" icon="/mxgraph/javascript/examples/editors/images/zoomactual.gif"/>
</mxDefaultToolbar>
