'******************************************************************************************************************	
	Public Function getDocUnidadPorCodigo(codigo As String) As NotesDocument
		On Error Goto HANDLER
		Dim view As NotesView
		Dim doc As NotesDocument
		Dim col As NotesDocumentCollection
		
		Set view = dbOrg.getView("(BusquedaUnixNro)")
		Set doc = view.GetDocumentByKey(codigo)
		
		Set getDocUnidadPorCodigo=doc
		Exit Function
HANDLER:
		Call RegistraryGenerarError(NOMBRE_APLICACION,"Clase Unidad - Método getDocUnidadPorCodigo","")
		Exit Function
	End Function
'******************************************************************************************************************	
	Public Function getSupervisorResol()
		On Error Goto HANDLER
		Dim doc As NotesDocument
		
		If Me.SupervisorResol ="" Then
			If docUnidad.GetItemValue("sUnidadSuperior")(0)<>"" Then
				Set doc = getDocUnidadPorCodigo(docUnidad.GetItemValue("sUnidadSuperior")(0))
				While Not doc Is Nothing
					If doc.GetItemValue(CAMPO_SUPERVISOR_RESOL)(0)<>"" Then
						getSupervisorResol=doc.GetItemValue(CAMPO_SUPERVISOR_RESOL)(0)
						Exit Function
					Else
						If doc.GetItemValue("sUnidadSuperior")(0)<>"" Then
							Set doc = getDocUnidadPorCodigo(doc.GetItemValue("sUnidadSuperior")(0))
						Else 
							getSupervisorResol=""
							Exit Function
						End If
					End If
				Wend
			Else
				getSupervisorResol=""
				Exit Function
			End If
		Else
			getSupervisorResol = Me.supervisorResol
		End If
		
		Exit Function
HANDLER:
		Call RegistraryGenerarError(NOMBRE_APLICACION,"Clase Unidad Class - Método getSupervisorResol","")
		Exit Function
	End Function
