'CONSTRUCTOR
	Public Sub new(Byval unidad As String)
		On Error Goto HANDLER
		Dim config As ConfigRD
		Dim server As String
		Dim path As String
		
		Set config = New ConfigRD
		server = config.Servidor(CLAVE_BASE_ORGANIZACION)
		path = config.BasedeDatos(CLAVE_BASE_ORGANIZACION)
		Set dbOrg = New NotesDatabase(server,path)
		
		sUnidad = unidad
		
		If c Is Nothing Then Set c = New ConfigRD
		
		If dbOrg Is Nothing Then _
		Error ERROR_DB_NOT_OPEN, MSG_ERROR_DB_NOT_OPEN & " " & CLAVE_BASE_ORGANIZACION
		
		Set ViewUnidades = dbOrg.GetView(VISTA_BUSQUEDAUNIDADES)
		If viewUnidades Is Nothing Then _
		Error ERROR_VIEW_NOT_FOUND,MSG_ERROR_VIEW_NOT_FOUND & " " & VISTA_BUSQUEDAUNIDADES 
		
		Set docUnidad = ViewUnidades.GetDocumentbyKey(sUnidad, True)
		If docUnidad Is Nothing Then
			'Por las dudas que me pasen el nro de unidad
			Set ViewUnidades = dbOrg.GetView(VISTA_BUSQUEDAUNIDADES_POR_NRO)
			If viewUnidades Is Nothing Then _
			Error ERROR_VIEW_NOT_FOUND,MSG_ERROR_VIEW_NOT_FOUND & " " & VISTA_BUSQUEDAUNIDADES 
			
			Set docUnidad = ViewUnidades.GetDocumentbyKey(sUnidad, True)	
			If Not docUnidad Is Nothing Then
				sUnidad = docUnidad.GetItemValue("sUnidad")(0)
			Else
				Msgbox "es nula la unidad"
			End If
		End If
		
		If docUnidad Is Nothing Then _
		Error ERROR_DOCUMENT_NOT_FOUND,MSG_ERROR_DOCUMENT_NOT_FOUND & " Unidad"
		
		Exit Sub
HANDLER:
		Call RegistraryGenerarError(NOMBRE_APLICACION,"Unidad - Constructor","")
		Exit Sub
	End Sub	
	
