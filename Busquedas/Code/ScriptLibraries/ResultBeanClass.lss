					strNumeroDoc = doc.Getitemvalue("ccnNroRes")(0)
				ElseIf doc.Hasitem("sNroSesion") Then
					strNumeroDoc = doc.Getitemvalue("sNroSesion")(0)& "#" & CStr(doc.Getitemvalue("ccnNroPunto")(0)) 
				End If	
				
